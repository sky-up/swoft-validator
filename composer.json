{
  "name": "swoft-fork/validator",
  "type": "library",
  "version": "v2.0.11",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "Validator component for swoft",
  "homepage": "https://github.com/sky-up/swoft-validator",
  "license": "Apache-2.0",
  "require": {
    "php": ">8.0",
    "swoft-fork/framework": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Validator\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Validator\\Unit\\": "test/unit",
      "SwoftTest\\Validator\\Testing\\": "test/testing"
    }
  },
  "scripts": {
    "test": "php run.php -c phpunit.xml"
  }
}
